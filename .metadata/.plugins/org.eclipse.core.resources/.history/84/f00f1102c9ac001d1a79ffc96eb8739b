package one.aula8;

import java.util.HashMap;
import java.util.Map;

public class InterfaceMap {
public static void main(String[] args) {
	Map<String,String>aluno = new HashMap<>();
	
	aluno.put("Name", "Joao");
	aluno.put("Idade", "17");
	aluno.put("Media", "8.5");
	aluno.put("Turma", "3a");
	
	System.out.println(aluno);
	
}
}
