package aula1;

public class AtrubuicaoReferencia {
	public static void main(String[] args) {
			
		int intA = 1;
		int intB = intA;
		
		String stA = new String( "Douglas");
		String stB = stA;
	
	System.out.println("stA: "+stA+", stA: "+stB);
	intA = 2;
	System.out.println("intA: "+intA+", intB: "+intB);
	}
}
